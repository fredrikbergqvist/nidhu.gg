<h2>Components</h2>
<article class="component-list">
	{%- for post in postslist | reverse %}
		<section class="component-item">
			<a class="" href="{{ post.url }}">
				<img src="{{ post.data.image }}" alt="{{ post.data.title }}" width="200" height="200" />
				<h3 class="sr-only">{% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</h3>
				<p class="sr-only">{{ post.data.description | safe }}</p>
			</a>
		</section>

	{%- endfor %}
</article>
