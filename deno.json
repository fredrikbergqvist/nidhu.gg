{
	"name": "@nidhugg/web-components",
	"version": "0.2.4",
	"exports": {
		".": "./dist/index.js",
		"./nidhuggModal": "./dist/modal/nidhuggModal.js",
		"./nidhuggImageModal": "./dist/imageModal/nidhuggImageModal.js",
		"./nidhuggBackToTop": "./dist/backToTop/nidhuggBackToTop.ts"
	},
	"author": "Fredrik Bergqvist",
	"description": "A no dependency web component library",
	"license": "MIT",
	"publish": {
		"include": [
			"LICENSE",
			"README.md",
			"dist/**/*"
		],
		"exclude": [
			"test/**/*"
		]
	},
	"tasks": {
		"build": "deno run --allow-read --allow-write --allow-env .\\build-dist.ts"
	},
	"imports": {
		"@std/assert": "jsr:@std/assert@1"
	},
	"compilerOptions": {
		"lib": [
			"dom",
			"esnext",
			"deno.ns"
		]
	}
}
